{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "deploy": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh",
    "backup": "chmod +x scripts/backup.sh && ./scripts/backup.sh",
    "monitor": "chmod +x scripts/monitor.sh && ./scripts/monitor.sh",
    "monitor:check": "chmod +x scripts/monitor.sh && ./scripts/monitor.sh check",
    "monitor:continuous": "chmod +x scripts/monitor.sh && ./scripts/monitor.sh continuous",
    "monitor:report": "chmod +x scripts/monitor.sh && ./scripts/monitor.sh report",
    "production:setup": "copy env.production.example .env.production && echo Configure .env.production antes de continuar",
    "production:build": "cross-env NODE_ENV=production npm run build",
    "production:start": "cross-env NODE_ENV=production npm start",
    "production:deploy": "npm run production:build && npm run production:start",
    "health:check": "powershell -Command \"try { Invoke-WebRequest -Uri 'http://localhost:3000/api/health' -UseBasicParsing | Out-Null; Write-Host 'Health check passed' } catch { Write-Host 'Health check failed' }\"",
    "logs:view": "powershell -Command \"Get-Content logs/app.log -Wait\"",
    "logs:clear": "if exist logs\\*.log del logs\\*.log",
    "clean": "if exist .next rmdir /s /q .next && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache && if exist .turbo rmdir /s /q .turbo",
    "reset": "npm run clean && npm install && npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:init": "node scripts/init-db.js",
    "db:studio": "prisma studio"
  },
